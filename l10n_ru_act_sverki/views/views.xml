<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_a4l" model="report.paperformat">
            <field name="name">A4 Landscape</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">7</field>
            <field name="margin_bottom">7</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>

        <record id="account_action_report_partnerledger_ru" model="ir.actions.report">
            <field name="name">Partner Ledger Ru</field>
            <field name="model">res.partner</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_ru_act_sverki.report_partnerledger_ru</field>
            <field name="report_file">l10n_ru_act_sverki.report_partnerledger_ru</field>
            <field name="print_report_name">"'ir.actions.act_window,' +str(ref('action_account_partner_ledger_filter_ru'))"</field>
            <field name="binding_model_id" ref="base.model_res_partner"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_a4l"/>
        </record>

        <record id="account_action_report_partnerledger_ru_without_stamp" model="ir.actions.report">
            <field name="name">Partner Ledger Ru Without Stamp</field>
            <field name="model">res.partner</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_ru_act_sverki.report_partnerledger_ru_without_stamp</field>
            <field name="report_file">l10n_ru_act_sverki.report_partnerledger_ru_without_stamp</field>
            <field name="print_report_name">"'ir.actions.act_window,' +str(ref('action_account_partner_ledger_filter_ru'))"</field>
            <field name="binding_model_id" ref="base.model_res_partner"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_a4l"/>
        </record>
    </data>
</odoo>